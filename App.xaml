             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ImageSource x:Key="TrayIcon">/Icons/app.ico</ImageSource>
        </ResourceDictionary>
    </Application.Resources>
</Application>
